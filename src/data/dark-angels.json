    "detachments": [
        {
            "id": "boarding_strike",
            "name": "Boarding Strike",
            "flavor_text": "Space Marines excel in the sort of fast-paced, ferociously violent battles that erupt during voidship boarding assaults.",
            "army_rule": {
                "name": "Oath of Moment",
                "description": "Select one unit to re-roll Hit rolls against.",
                "replaces": "Standard Oath"
            },
            "detachment_rule": {
                "name": "Drive Home the Blade",
                "description": "Each time a unit shoots, if within 1 inch of an open Hatchway and every model is on same side, select enemy on opposite side. Weapons gain [SUSTAINED HITS 1] vs that enemy."
            },
            "mustering_rules": [
                {
                    "type": "select_up_to_x",
                    "category_name": "Leaders",
                    "limit": 2,
                    "allowed_units": ["sm_captain", "da_lion"],
                    "exclusions": [
                        { "unit_id": "sm_captain", "forbidden_keyword": "Terminator" }
                    ]
                },
                {
                    "type": "select_up_to_x_each",
                    "category_name": "Troops & Battleline",
                    "limit_per_unit": 3,
                    "allowed_units": ["sm_intercessor", "sm_assault_int"]
                },
                {
                    "type": "select_up_to_x_each",
                    "category_name": "Elites",
                    "limit_per_unit": 1,
                    "allowed_units": ["da_inner_circle", "sm_bladeguard", "sm_aggressor", "sm_infernus", "sm_sternguard"]
                },
                {
                    "type": "select_up_to_x_each",
                    "category_name": "Heavy Support",
                    "limit_per_unit": 1,
                    "allowed_units": ["sm_hellblaster", "sm_eradicator"]
                }
            ],
            "rules_adaptations": [
                { "unit_id": "sm_intercessor", "remove_ability": "Objective Secured" }
            ],
            "enhancements": [
                { "name": "Adamantine Mantle", "description": "Subtract 1 from Damage characteristic of attacks allocated to bearer." },
                { "name": "Clavitine Reliquary", "description": "In Movement phase, if within 1 inch of Hatchway, can operate it. Can only operate once per turn." }
            ],
            "stratagems": [
                { "name": "In The Emperor's Name", "cost": 1, "description": "Target Battleline unit gains Feel No Pain 5+." },
                { "name": "Ceramite Bulwark", "cost": 1, "description": "Add 1 to Attacks and Strength of melee weapons. Must target enemy on opposite side of Hatchway." },
                { "name": "Duty and Defiance", "cost": 1, "description": "Fight on Death on a 4+ (add +1 to roll if Battleline)." },
                { "name": "Vox-Amplified Roar", "cost": 1, "description": "Charge target must take Battle-shock test." }
            ]
        },
